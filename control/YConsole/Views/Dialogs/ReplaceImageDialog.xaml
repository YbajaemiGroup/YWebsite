<UserControl x:Class="YConsole.Views.Dialogs.ReplaceImageDialog"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:YConsole.Views.Dialogs"
             mc:Ignorable="d" 
             d:DesignHeight="200" d:DesignWidth="420"
             Height="200" Width="420">
    <Grid Background="White">
        <TextBlock Text="{Binding QuestionText}" Margin="147,45,147,119"></TextBlock>
        <Button Content="Заменить" Style="{StaticResource DefaultButtonStyle}" Height="30" Margin="10,124,0,0" Width="185" Click="Button_Click"></Button>
        <Button Content="Отмена" Style="{StaticResource DefaultButtonStyle}" Height="30" Margin="225,124,0,0" Width="185" Click="Button_Click_1"></Button>
    </Grid>
</UserControl>
